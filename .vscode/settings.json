{
  "java.compile.nullAnalysis.mode": "disabled",
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.autobuild.enabled": false,
  "java.maxConcurrentBuilds": 1,
  "gradle.nestedProjects": false,
  "android.gradle.buildTasks": [],
  "files.exclude": {
    "**/android/.gradle": true,
    "**/android/build": true,
    "**/android/gradle": false,
    "**/android/gradlew": false,
    "**/android/gradlew.bat": false,
    "**/build": true,
    "**/.dart_tool": true
  },
  "files.watcherExclude": {
    "**/android/.gradle/**": true,
    "**/android/build/**": true,
    "**/build/**": true,
    "**/.dart_tool/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    "**/android/.gradle": true,
    "**/android/build": true,
    "**/build": true,
    "**/.dart_tool": true
  }
}
